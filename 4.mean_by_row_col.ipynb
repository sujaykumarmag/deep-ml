   "source": [
    "def calculate_matrix_mean(matrix: list[list[float]], mode: str) -> list[float]:\n",
    "\tif mode == \"column\":\n",
    "\t\tmatrix = [list(x) for x in zip(*matrix)]\n",
    "\treturn [sum(i)/len(i) for i in matrix]"
   ]
  },
